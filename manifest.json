{
  "manifest_version": 2,
  "name": "Tg Helper",
  "description": "Написание сообщений в телеграм",
  "version": "1.0.0",
  "icons": { "16": "icon_16.png", "48": "icon_48.png", "128": "icon_128.png" },
  "browser_action": {
    "default_icon": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "default_title": "Tg Helper",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "clipboardRead", "clipboardWrite", "tabs"]
}
